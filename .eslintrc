{
  "env": {
    "node": true
  },
  "globals": {
    "Promise": true
  },
  "rules": {
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "dot-notation": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-eq-null": 2,
    "no-multi-str": 2
  }
}
